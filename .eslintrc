{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allowAfterThis": true
      }
    ],
    "camelcase": [
      0,
      {
        "properties": "never"
      }
    ],
    "no-unused-vars": [
      "error",
      {
        "args": "none"
      }
    ]
  }
}
